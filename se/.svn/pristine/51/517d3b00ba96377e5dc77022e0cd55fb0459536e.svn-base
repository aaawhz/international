html{_overflow-x:hidden;}
body{overflow-x:hidden;}
#container{_display:inline;width:806px;margin:0 auto;text-align:left;}
.info, .news, .bulletin, .links{display:inline;float:left;margin:5px 0 0 5px;}
.info{width:315px;height:263px;border:1px #b3c4db solid;background-image:url('../images/sfz/sfz_bg.gif');background-repeat:repeat-x;background-position:0 -1px;}
.info h2{height:45px;line-height:45px;margin-left:5px;font-size:14px;color:#0a49a2;}
.info h2 strong{font-weight:bold;color:#e8713f;}
.info img{float:left;margin:0 10px;}
.infoC{float:left;display:inline;width:160px;}
.info em{display:block;height:22px;padding:20px 0 0 50px;background-image:url('../images/sfz/sfz.gif');background-repeat:no-repeat;background-position:-300px -60px;font-size:14px;color:white;font-weight:bold;}
.info p{margin-top:10px;line-height:2;}
.info ul{clear:both;padding:15px 0 0;}
.info ul li{float:left;display:inline;width:80px;height:33px;padding-top:7px;margin:0 12px;background-image:url('../images/sfz/sfz.gif');text-align:center;font-weight:bold;}
.info ul li a{display:block;height:100%;color:#1774e1;}
.info ul li a:hover{color:#e8713f;}
.news{width:465px;height:257px;padding:4px;background-color:#eaf0f3;}
.news ul{width:464px;height:26px;border-top:1px #b3c4db solid;border-left:1px #b3c4db solid;}
.news li{float:left;display:inline;height:25px;line-height:25px;width:115px;border-right:1px #b3c4db solid;border-bottom:1px #b3c4db solid;background-color:#f5f5f5;text-align:center;font-weight:bold;}
.news li a{display:block;height:25px;color:#0a49a2;}
.news li a:hover{color:#e8713f;}
.news li.on{height:26px;border-bottom:0;background-color:white;}
.news iframe{display:block;width:463px;_width:461px;height:229px;border-right:1px #b3c4db solid;border-bottom:1px #b3c4db solid;border-left:1px #b3c4db solid;}
.bulletin{width:317px;height:160px;background-image:url('../images/sfz/sfz.gif');background-position:0 -308px;background-repeat:no-repeat;}
.bulletin h2{height:42px;background-image:url('../images/sfz/sfz.gif');background-position:-40px -260px;background-repeat:no-repeat;text-indent:-9999px;}
.bulletin i{float:left;width:59px;height:69px;margin:0 10px;border:1px #ccc solid;background-image:url('../images/sfz/sfz.gif');background-repeat:no-repeat;background-position:-40px -180px;}
.bulletin ul{float:left;display:inline;width:210px;line-height:1.5;}
.links{width:430px;height:120px;padding:40px 20px 0 10px;background-image:url('../images/sfz/sfz.gif');background-position:0 -499px;background-repeat:no-repeat;}
.links h2{display:none;}
.links a.icon{float:left;width:206px;height:65px;background-image:url('../images/sfz/sfz.gif');background-repeat:no-repeat;background-position:-120px -180px;}
.links p{float:left;display:inline;margin-left:10px;width:210px;line-height:1.5;}
.links .hr{clear:both;height:0;overflow:hidden;padding-top:7px;margin-bottom:7px;border-bottom:1px #ccc dotted;font-size:0;}
.links .hr hr{display:none;}
.links fieldset select{width:100px;margin:0 2px;}
a.banner{clear:both;display:block;height:155px;background-repeat:no-repeat;background-image:url('../images/sfz/sfz_jdl.gif');background-position:5px bottom;}
.footer{padding:10px;text-align:center;line-height:2;}
.footer, .footer a{color:#7f7f7f;}
.footer a{text-decoration:underline;}
